<fx:root alignment="TOP_RIGHT" fillHeight="false" maxHeight="1.7976931348623157E308" maxWidth="-Infinity" prefWidth="400.0" type="HBox" xmlns="http://javafx.com/javafx/21.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="dialog" maxHeight="1.7976931348623157E308" text="Label" textOverrun="CLIP" wrapText="true" HBox.hgrow="ALWAYS">
